<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <Optimize>True</Optimize>
    <NoWarn>618;1591</NoWarn>
    <DefineConstants>
    </DefineConstants>
    <OutputPath>bin\Debug\</OutputPath>
    <DocumentationFile>bin\Debug\GoogleApi.XML</DocumentationFile>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <Description>Google Places, Maps, Roads, Search and Translate integrations. Requests and Responses are complete with Google Api documentation.</Description>
    <Product>GoogleApi</Product>
    <Company />
    <Authors>Michael Vivet</Authors>
    <Version>2.5.0</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageTags>google api map maps place places elevation snaptoroad snaptoroads snap road roads speed speedlimit coordinate geocode geocoder geocoding search text nearby radar translate translation geolocation nearest geography point geocoordinate address location latitude longitude distance duration matrix distancematrix direction directions travel path journey trip photo photos timezone time zone autocomplete auto-complete</PackageTags>
    <PackageReleaseNotes>
    </PackageReleaseNotes>
    <PackageLicenseUrl>https://github.com/vivet/GoogleApi/blob/master/UNLICENSE.txt</PackageLicenseUrl>
    <Copyright>
    </Copyright>
    <PackageProjectUrl>https://github.com/vivet/GoogleApi</PackageProjectUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <PackageIconUrl>https://github.com/vivet/GoogleApi/blob/master/icon.ico</PackageIconUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>
</Project>